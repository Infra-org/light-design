.fixed_dialog_wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}
.fixed_dialog_wrap .dialog_wrap {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.fixed_dialog_wrap .dialog_wrap .mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.fixed_dialog_wrap .dialog_wrap .dialog {
  display: flex;
}
.fixed_dialog_wrap .dialog_wrap .dialog.top {
  position: absolute;
  top: 0;
}
.fixed_dialog_wrap .dialog_wrap .dialog.bottom {
  position: absolute;
  bottom: 0;
}
.fixed_dialog_wrap .dialog_wrap .dialog.left {
  position: absolute;
  left: 0;
}
.fixed_dialog_wrap .dialog_wrap .dialog.right {
  position: absolute;
  right: 0;
}
