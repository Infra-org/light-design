@import './styles/atom-miniapp.min.wxss';
@import './styles/init.wxss';
@import './styles/vars.wxss';
page {
  background-color: var(--color_background);
  overflow-x: hidden;
}
.mini_items {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  border-top: 2rpx solid var(--color_border);
}
.mini_items.no_border {
  border-top: none;
}
.mini_items.column_2 .example_item {
  width: 50%;
}
.mini_items.column_2 .example_item:nth-of-type(2n) {
  border-right: none;
}
.mini_items.column_2 .example_item:nth-of-type(3n) {
  border-right: 2rpx solid var(--color_border);
}
.mini_items.column_2 .example_item .examples {
  flex-direction: column;
  justify-content: center;
}
.mini_items.column_2 .example_item .examples .example {
  align-items: center;
}
.mini_items.visible_items .example_item {
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.mini_items.visible_items .example_item .title_wrap {
  margin-bottom: 0;
}
.mini_items .example_item {
  width: 33.33333333%;
  padding: 60rpx 48rpx;
  border-right: 2rpx solid var(--color_border);
}
.mini_items .example_item:nth-of-type(3n) {
  border-right: none;
}
.mini_items .example_item:last-child {
  border-bottom: 2rpx solid var(--color_border);
}
.mini_items .example_item .title_wrap {
  flex-direction: column;
}
.mini_items .example_item .title_wrap .title {
  font-size: 24rpx;
  margin-bottom: 12rpx;
}
.mini_items .example_item .examples {
  height: 100%;
}
.mini_items .example_item .examples .example {
  padding: 12rpx 0;
}
