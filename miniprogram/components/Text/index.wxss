.init {
  font-size: 30rpx;
  line-height: 1.6;
  color: black;
}
.text_wrap {
  font-size: 30rpx;
  line-height: 1.6;
  color: black;
}
.text_wrap.p {
  display: inline-block;
  margin: 16rpx 0;
  line-height: 1.8;
}
.text_wrap.b {
  font-weight: bold;
}
.text_wrap.i {
  font-style: italic;
}
.text_wrap.del {
  text-decoration: line-through;
}
.text_wrap.h7 {
  font-size: 0.6em;
}
.text_wrap.h6 {
  font-size: 0.8em;
}
.text_wrap.h5 {
  font-size: 1em;
}
.text_wrap.h4 {
  font-size: 1.2em;
}
.text_wrap.h3 {
  font-size: 1.5em;
}
.text_wrap.h2 {
  font-size: 1.8em;
}
.text_wrap.h1 {
  font-size: 2.1em;
}
.text_wrap.quote {
  box-sizing: border-box;
  padding: 24rpx;
  background-color: whitesmoke;
  font-size: 0.9em;
  color: #888;
}
.text_wrap.success {
  color: #81C784;
}
.text_wrap.warning {
  color: #FFB74D;
}
.text_wrap.error {
  color: #E57373;
}
.text_wrap.line_clamp_1 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 1;
}
.text_wrap.line_clamp_2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 2;
}
.text_wrap.line_clamp_3 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 3;
}
.text_wrap.line_clamp_4 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 4;
}
.text_wrap.line_clamp_5 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 5;
}
