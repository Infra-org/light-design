@keyframes light {
  from {
    clip: rect(60rpx 60rpx 60rpx 0);
  }
  to {
    clip: rect(0 60rpx 60rpx 0);
  }
}
.landing_wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.landing_wrap .landing {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.9);
  border-radius: 24rpx;
}
.landing_wrap .landing .icon {
  width: 60rpx;
  height: 60rpx;
}
.landing_wrap .landing .icon_source {
  width: 60rpx;
  height: 60rpx;
  background-size: 100% 100%;
  transform-origin: center bottom;
  background-image: url('data:image/svg+xml;base64,');
}
.landing_wrap .landing .icon_source_bottom {
  position: absolute;
  opacity: 0.3;
}
.landing_wrap .landing .icon_source_top {
  position: absolute;
  animation: light 1s infinite;
}
