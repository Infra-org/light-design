.fixed_dialog_wrap .dialog {
  position: fixed;
  left: 0;
  box-sizing: border-box;
  display: flex;
  width: 100%;
  justify-content: center;
  transition: all ease 0.3s;
}
.fixed_dialog_wrap .dialog .content_wrap {
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 24rpx 30rpx;
  border-radius: 100rpx;
  background-color: #222;
  color: white;
  font-size: 26rpx;
  line-height: 1;
}
.fixed_dialog_wrap .dialog .content_wrap .icon {
  width: calc(18rpx * 2);
  height: calc(18rpx * 2);
  margin-right: 16rpx;
}
.fixed_dialog_wrap .dialog .content_wrap.success {
  color: white;
}
.fixed_dialog_wrap .dialog .content_wrap.success.useColor {
  background-color: #52c41a;
}
.fixed_dialog_wrap .dialog .content_wrap.success .icon {
  background-image: url('data:image/svg+xml;base64,');
}
.fixed_dialog_wrap .dialog .content_wrap.warning {
  color: white;
}
.fixed_dialog_wrap .dialog .content_wrap.warning.useColor {
  background-color: #faad14;
}
.fixed_dialog_wrap .dialog .content_wrap.warning .icon {
  background-image: url('data:image/svg+xml;base64,');
  background-size: cover;
}
.fixed_dialog_wrap .dialog .content_wrap.error {
  color: white;
}
.fixed_dialog_wrap .dialog .content_wrap.error.useColor {
  background-color: #d62d0e;
}
.fixed_dialog_wrap .dialog .content_wrap.error .icon {
  background-image: url('data:image/svg+xml;base64,');
}
