<view class="example_items w_100 border_box flex flex_column">
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">默认效果</text>
                  <text class="attr">default</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="default"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_default}}"
                              title="分享"
                              list="{{list}}"
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">没有标题</text>
                  <text class="attr">title</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="title"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_title}}"
                              list="{{list}}"
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">横向滚动</text>
                  <text class="attr">scroll</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="scroll"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_scroll}}"
                              title="分享"
                              list="{{list}}"
                              scroll
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">纵向显示</text>
                  <text class="attr">vertical</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="vertical"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_vertical}}"
                              title="分享"
                              list="{{list}}"
                              vertical
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">显示取消</text>
                  <text class="attr">visibleCancel</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="visibleCancel"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_visibleCancel}}"
                              list="{{list}}"
                              vertical
                              visibleCancel
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">行个数</text>
                  <text class="attr">column</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="column"
                              catchtap="onOption"
                        >Show</_Button>
                        <_ActionSheet
                              visible="{{visible_column}}"
                              title="分享"
                              list="{{list}}"
                              column="{{4}}"
                        ></_ActionSheet>
                  </view>
            </view>
      </view>
</view>