page {
  background-color: transparent;
}
.menu_items_wrap {
  padding: 36px;
}
.menu_items_wrap .menu_items .menu_item {
  margin-bottom: 36px;
}
.menu_items_wrap .menu_items .menu_item .title_wrap .title {
  flex: 1;
  padding: 12px 0;
  color: var(--color_text_sub);
}
.menu_items_wrap .menu_items .menu_item .title_wrap .icon_close {
  display: none;
}
.menu_items_wrap .menu_items .menu_item .components .component {
  width: calc((100% - 24rpx) / 2);
  padding: 32rpx;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
  border-top-right-radius: 36rpx;
  border-bottom-left-radius: 36rpx;
  box-shadow: var(--box_shadow_morphic);
  background-color: white;
}
.menu_items_wrap .menu_items .menu_item .components .component:nth-of-type(2n) {
  margin-right: 0;
}
.menu_items_wrap .menu_items .menu_item .components .component.navigator-hover {
  opacity: 1;
  background-color: white;
  box-shadow: var(--box_shadow_morphic_active);
}
.menu_items_wrap .menu_items .menu_item .components .component .description {
  font-size: 26rpx;
  color: var(--color_text_desc);
}
