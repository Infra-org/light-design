                  </view>
                  <view class="examples flex align_center">
                        <view class="example flex">
                              <light-line
                                    class="mr_24"
                                    radius="{{8}}"
                                    box
                                    thin
                                    catchtap="click"
                              >
                                    <light-button ghost>Primary</light-button>
                              </light-line>
                        </view>
